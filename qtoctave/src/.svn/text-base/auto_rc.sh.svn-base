echo "<!DOCTYPE RCC><RCC version=\"1.0\">" > images.qrc
echo "<qresource>" >> images.qrc
	
for a in `ls images`; do
	echo "<file>images/$a</file>"  >> images.qrc;
done

echo "</qresource></RCC>"  >> images.qrc

